{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg",
          "**/*.gif",
          "**/*.svg",
          "**/includes/media/**"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/*.md"
        ]
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "breadcrumb_path": "/power-bi/breadcrumb/toc.json",
      "uhfHeaderId": "MSDocsHeader-MSPowerBI",
      "show_latex": true,
      "contributors_to_exclude": [
        "openpublishingbuild",
        "sudeepku",
        "v-thepet"
      ],
      "searchScope": [
        "Power BI"
      ],
      "titleSuffix": "Power BI",
      "feedback_system": "GitHub",
      "feedback_github_repo": "MicrosoftDocs/powerbi-docs",
      "feedback_product_url": "https://ideas.powerbi.com/forums/265200-power-bi",
      "search.appverid": "met150",
      "search.app": [
        "capaedac-PowerBI",
        "powerbi",
        "powerbipremium-docs,
        "powerbipro-docs"]
    },
    "fileMetadata": {
          "featureFlags": {
              "**/*.md": [
                 "show_binary_rating"
              ]
          }
    },
    "template": [],
    "dest": "powerbi-docs",
    "markdownEngineName": "markdig",
    "xrefService": [ "https://xref.learn.microsoft.com/query?uid={uid}" ]
  }
}
