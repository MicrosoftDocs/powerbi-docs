{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg",
          "**/*.gif",
          "**/*.svg",
          "**/includes/media/**"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/*.md"
        ]
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "breadcrumb_path": "/power-bi/breadcrumb/toc.json",
      "uhfHeaderId": "MSDocsHeader-MSPowerBI",
      "show_latex": true,
      "contributors_to_exclude": [
        "openpublishingbuild",
        "sudeepku",
        "v-thepet"
      ],
      "searchScope": [
        "Power BI"
      ],
      "titleSuffix": "Power BI",
      "feedback_system": "Standard",
      "feedback_product_url": "https://ideas.fabric.microsoft.com/?forum=2d80fd4a-16cb-4189-896b-e0dac5e08b41",
      "feedback_help_link_url": "https://community.fabric.microsoft.com/powerbi",
      "feedback_help_link_type": "ask-the-community",
      "feedback_github_repo": "MicrosoftDocs/powerbi-docs",
      "search.appverid": "met150",
      "search.app": [
        "fabric-powerbi-docs",
        "capaedac-PowerBI",
        "powerbi",
        "powerbipremium-docs",
        "powerbipro-docs"]
    },
    "fileMetadata": {
          "featureFlags": {
              "**/*.md": [
                 "show_binary_rating"
              ]
          },
          "ms.subservice": {
              "collaborate-share/**/*.md": "pbi-collaborate-share",
              "connect-data/**/*.md": "pbi-data-sources",
              "consumer/**/*.md": "pbi-explore",
              "create-reports/**/*.md": "pbi-reports-dashboards",
              "developer/**/*.md": "powerbi-developer",
              "enterprise/**/*.md": "powerbi-admin",
              "fundamentals/**/*.md": "pbi-fundamentals",
              "guidance/**/*.md": "admin",
              "guided-learning/**/*.md": "*",
              "learning-catalog/**/*.md": "*",
              "natural-language/**/*.md": "powerbi-ai",
              "paginated-reports/**/*.md": "pbi-reports-dashboards",
              "report-server/**/*.md": "powerbi-report-server",
              "support/**/*.md": "pbi-troubleshooting",
              "transform-model/**/*.md": "pbi-transform-model",
              "troubleshoot/**/*.md": "*",
              "visuals/**/*.md": "pbi-visuals"
          }
    },
    "template": [],
    "dest": "powerbi-docs",
    "markdownEngineName": "markdig",
    "xrefService": [ "https://xref.learn.microsoft.com/query?uid={uid}" ]
  }
}
